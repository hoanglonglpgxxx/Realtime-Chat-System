{
  "name": "realtime-chat-system",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "fe": "dotenv -e .env -- cross-var \"npm run dev -w fe -- -p %FE_PORT%\"",
    "be": "dotenv -e .env -- npm run dev -w be",
    "dev:all": "npm-run-all --parallel --print-label fe be"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "cross-var": "^1.1.0",
    "dotenv-cli": "^11.0.0"
  }
}